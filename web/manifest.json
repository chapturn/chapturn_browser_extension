{
    "name": "ChapturnExt",
    "description": "A new Flutter project.",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "/icons/Icon-192.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/content.js"]
        }
    ],
    "permissions": ["downloads", "webRequest", "tabs"],
    "host_permissions": ["<all_urls>"],
    "incognito": "split",
    "manifest_version": 3
}
